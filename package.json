{
  "name": "@telerik/kendo-intl",
  "description": "A package exporting functions for date and number parsing and formatting",
  "version": "0.0.0-semantically-released",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-intl.git"
  },
  "cldr-data-coverage": "full",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "gulp test",    
    "start": "gulp start",
    "lint": "gulp lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "Kendo",
    "Internationalization"
  ],
  "dependencies": {
    "@telerik/eslint-config": "1.1.0",
    "@telerik/kendo-common-tasks": "^1.8.0",
    "babel-core": "~6",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-umd": "~6",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "~6",    
    "babel-preset-stage-1": "~6",
    "babel-traverse": "6.3.19",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "cldr-data": "latest",    
    "extract-text-webpack-plugin": "^0.9.1",
    "gulp": "3.9.0",
    "gulp-jasmine": "^2.2.1",
    "jasmine-reporters": "^2.1.1",
    "jasmine-spec-reporter": "^2.4.0",    
    "json-loader": "^0.5.4",
    "source-map-loader": "^0.1.5"
  },
  "author": "Telerik",
  "license": "Apache-2.0",
  "devDependencies": {
    "cldr-data": "latest",
    "cz-conventional-changelog": "^1.1.5",
    "ghooks": "^1.0.3",
    "gulp": "3.9.0",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    }
  }
}
